@using MembershipApps.Areas.Admin.Models
@model IEnumerable<Memberships.Entities.ProductLinkText>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.Partial("_CreateButtonPartial")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        @Html.Partial("_TableButtonPartial", new SmallButtonModel { Id = item.Id})
    </tr>
}

</table>
